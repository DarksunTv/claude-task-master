{
	"mcpServers": {
		"task-master-ai": {
			"command": "npx",
			"args": ["-y", "--package=task-master-ai@latest", "task-master-ai"],
			"env": {
				"ANTHROPIC_API_KEY": "YOUR_ANTHROPIC_API_KEY_PLACEHOLDER",
				"PERPLEXITY_API_KEY": "YOUR_PERPLEXITY_API_KEY_PLACEHOLDER",
				"OPENAI_API_KEY": "YOUR_OPENAI_API_KEY_PLACEHOLDER",
				"GOOGLE_API_KEY": "YOUR_GOOGLE_API_KEY_PLACEHOLDER",
				"XAI_API_KEY": "YOUR_XAI_API_KEY_PLACEHOLDER",
				"OPENROUTER_API_KEY": "YOUR_OPENROUTER_API_KEY_PLACEHOLDER",
				"MISTRAL_API_KEY": "YOUR_MISTRAL_API_KEY_PLACEHOLDER",
				"AZURE_OPENAI_API_KEY": "YOUR_AZURE_OPENAI_API_KEY_PLACEHOLDER",
				"OLLAMA_API_KEY": "YOUR_OLLAMA_API_KEY_PLACEHOLDER"
			},
			"type": "stdio"
		}
	}
}
